{
	"name": "Parquet1",
	"properties": {
		"linkedServiceName": {
			"referenceName": "clasedfactorydatos",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"carpeta": {
				"type": "string"
			},
			"carpeta2": {
				"type": "string"
			},
			"nombre_archivo": {
				"type": "string"
			}
		},
		"annotations": [],
		"type": "Parquet",
		"typeProperties": {
			"location": {
				"type": "AzureBlobFSLocation",
				"fileName": {
					"value": "@concat(dataset().nombre_archivo, '_', formatDateTime(utcnow()))",
					"type": "Expression"
				},
				"folderPath": {
					"value": "@dataset().carpeta2",
					"type": "Expression"
				},
				"fileSystem": {
					"value": "@dataset().carpeta",
					"type": "Expression"
				}
			},
			"compressionCodec": "snappy"
		},
		"schema": []
	}
}